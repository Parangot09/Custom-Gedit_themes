<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="Crisp_theme" _name="Crisp" version="1.0">
  <author>Dheeraj Parangot</author>
  <_description>Crisp good looking</_description>
  
  <style background="#4d4d4d" name="current-line"/>
	<style background="#857b6f" bold="true" foreground="#f6f3e8" name="bracket-match"/>
	<style background="#656565" name="cursor"/>
	<style background="#000612" foreground="#f6f3e8" name="text"/>
	<style background="#000000" foreground="#857b6f" name="line-numbers"/>
	<style foreground="#3a86ff" italic="true" name="def:comment"/>
	<style foreground="#f7768e" name="def:constant"/>
	<style foreground="#9ece6a" italic="false" name="def:string"/>
	<style foreground="#8093f1" name="def:identifier"/>
	<style foreground="#8093f1" name="def:function"/>
	<style foreground="#8093f1" name="def:type"/>
	<style foreground="#edafb8" name="def:statement"/>
	<style foreground="#d90429" name="def:keyword" bold = "true"/>
	<style foreground="#f7768e" name="def:preprocessor"/>
	<style foreground="#f7768e" name="def:number"/>
	<style foreground="#e7f6da" name="def:specials"/>
	
 

  </style-scheme>

